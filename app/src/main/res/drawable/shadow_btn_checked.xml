<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/seat_color">
        <shape android:shape="rectangle">
            <corners android:radius="5dp"/>
            <solid android:color="@color/colorGrayBasic" android:width="2dp"/>
        </shape>
    </item>

    <item android:id="@+id/seat_shape" android:right="5dp" android:left="5dp" android:top="5dp" android:bottom="5dp">
        <shape  android:shape="rectangle">
            <stroke android:color="@color/colorBlue" android:width="1dp" />
            <corners android:bottomLeftRadius="10dp"/>
            <solid android:color="@color/colorYellow" android:alpha="0.5"/>
            <padding android:bottom="3dp" android:left="3dp" android:right="3dp" android:top="3dp"/>
        </shape>
    </item>

</layer-list>